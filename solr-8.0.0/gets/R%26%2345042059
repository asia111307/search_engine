<script>
    $(function () {
        setTimeout(function() {
            $('.foggy-image').each(function (i, img) {
                var canvas;

                function hasCssFilterSupport() {
                    var el = document.createElement('div');
                    el.style.cssText = '-webkit-filter:blur(2px);filter:blur(2px);';
                    return !!el.style.length && ((document.documentMode === undefined || document.documentMode > 9));
                }

                if (!hasCssFilterSupport()) {
                    canvas = document.createElement('canvas');
                    canvas.id = 'stackBlurCanvas' + img.src + i;
                    canvas.className = 'stack-blur-canvas';
                    canvas.style.position = 'absolute';
                    canvas.style.left = '0';
                    canvas.style.right = '0';
                    canvas.style.top = '0';
                    canvas.style.bottom = '0';
                    canvas.style.minWidth = '100%';
                    canvas.style.minHeight = '100%';
                    canvas.style.maxWidth = '100%';
                    canvas.style.maxHeight = '100%';
                    canvas.style.opacity = '.8';
                    img.id = img.id || ('stackBlurImage' + img.src + i);
                    $(img).after(canvas);
                    stackBlurImage(img.id, canvas.id, 50, false);
                    img.style.opacity = '0';
                }
            });
        }, 1000);
    });
    $(document).ready(function() {
		if($('#salaryPredictionTooltip').val() == "true") {
			var data = {};
			var count = 1;
			var pcip = $('#pcip').val();
			if (localStorage.getItem('spWidgetTooltip'+pcip)) {
				data = JSON.parse(localStorage.getItem('spWidgetTooltip'+pcip));
				data.count = data.count + 1;
				count = data.count;
				localStorage.setItem('spWidgetTooltip'+pcip, JSON.stringify(data));
			} else {
				localStorage.setItem('spWidgetTooltip'+pcip, JSON.stringify({"count": 1, "date": new Date()}));
			}
			if (count <= 5) {
				$('#popoverTipMob').popover({
					content: '<div class="row"><div class="col-sm-10 col-xs-10" style="padding-right: 0px;"><div class="icon-lightbulb-4" style="float: left;margin-right: 10px;"></div><div style="line-height: 18px;margin-top: 5px;color: #000;font-size: 16px;">Depicts typical salary range for this type of job and location.</div></div><div class="col-sm-2 col-xs-2 close" style="opacity: 0.5 !important;padding-left: 5px;"><i class="icon icon-delete-3" style="color: #000 !important;"></i></div></div>',
					html: true,
					trigger: 'hover',
					placement:'top'
				}).popover('show');
				$('.close').click(function(){
					$('[data-original-title]').popover('hide');
				});

				$('#popoverTip').popover({
					content: '<div class="row"><div class="col-lg-12 col-md-12"><div class="icon-lightbulb-4" style="float: left;margin-left: 10px;margin-right: 10px;"></div><div style="line-height: 18px;margin-top: 5px;font-size: 18px;color: #000" >Depicts typical salary range for this type of job and location.</div></div></div>',
					html: true,
					trigger: 'hover',
					placement:'left'
				}).popover('show');
				$('.close').click(function(){
					$('[data-original-title]').popover('hide');
				});
			}
		}
	});
</script>


	
