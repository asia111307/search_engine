mage/jpeg","media_details":{"width":1620,"height":1620,"hwstring_small":"height='1620' width='1620'","file":"cognitive-design-sq.jpg","sizes":{}},"source_url":"https://s3.amazonaws.com/www-inside-design/uploads/2018/03/cognitive-design-sq.jpg","featured":"https://s3.amazonaws.com/www-inside-design/uploads/2018/03/cognitive-design-sq.jpg"}],"wp:term":[[{"id":473,"link":"https://www.invisionapp.com/inside-design/category/design/","name":"Design","slug":"design","taxonomy":"category","yoast":{"title":"","metadesc":null,"opengraph-title":null,"opengraph-description":null,"opengraph-image":null,"twitter-title":null,"twitter-description":null,"twitter-image":null}}],[{"id":28,"link":"https://www.invisionapp.com/inside-design/tag/featured-home/","name":"featured-home","slug":"featured-home","taxonomy":"post_tag"}]]}},"__typename":"Post","stickyPosts":null},"$Post:34033.title":{"rendered":"What cognitive science taught me about design","__typename":"Content"},"$Post:34033.excerpt":{"rendered":"\u003cp\u003eSome design is just for design\u0026#8217;s sake. But designing for usability relies heavily on an understanding of cognitive science.\u003c/p\u003e\n","__typename":"Content"}}},"page":"/single","query":{"slug":"cognitive-science-design"},"buildId":"482d23ad1b1f9257b71f859de87a7b70fc6209e8","assetPrefix":"/inside-design","runtimeConfig":{"isDev":false,"assetPrefix":"/inside-design","PISTANOS":"PRODUCTION","PISTANOS_URL":"https://s3.amazonaws.com/www.invisionapp.com/Pistanos/pistanos.min.js","CMS":"http://invision-www-blog-cms-svc","S3":"https://s3.amazonaws.com/www-inside-design","PERSONALIZATION_HOST":"https://personalization.invisionapp.com","SITE_ROOT":"/inside-design","SERVER":"http://localhost","NR_BROWSER_LICENSE_KEY":"1b9c51ab63","NR_BROWSER_APPID":"179095668"}};__NEXT_LOADED_PAGES__=[];__NEXT_REGISTER_PAGE=function(r,f){__NEXT_LOADED_PAGES__.push([r, f])}</script><script async="" id="__NEXT_PAGE__/single" src="/inside-design/_next/static/482d23ad1b1f9257b71f859de8