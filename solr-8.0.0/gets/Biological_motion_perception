                      <mo stretchy="false">)</mo>
                    </mrow>
                    <mo>|</mo>
                  </mrow>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mn>2</mn>
                  </mrow>
                </msup>
                <mrow>
                  <mn>2</mn>
                  <mo>&#x22C5;<!-- ⋅ --></mo>
