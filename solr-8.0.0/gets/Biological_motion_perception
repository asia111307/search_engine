        <mi>t</mi>
        <mo stretchy="false">)</mo>
        <mo>+</mo>
        <munder>
          <mo>&#x2211;<!-- ∑ --></mo>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>k</mi>
          </mrow>
        </munder>
        <msubsup>
          <mi>H</mi>
          <mrow class="MJX-TeXAtom-ORD">
