<script>


	// this global var contains all the results of the active search
	var current_users = [];

	loader.ready(function() {
		
		var socket = io.connect('http://'+document.location.host+":8003");
		
		// Lets log the id for debugging purposes.
		socket.on("id", function(data) {
			if(window.console && console.log)
				console.log(data);
		})
		
		if($("html.ie7").length > 0) {
			$(".online_media .icon").html("");
			$(".online_media").removeClass("word");
		}
		
		if(navigator.userAgent.match(/iPad|iPhone|Android|IEMobile|BlackBerry/i)) {
			$("select.media, select.interest").show();
		} else {
			$(".basic SELECT").selectBox();	
		}
		
		$(".pagination:eq(1)").click(function() { 
			$("html,body").scrollTop(90); $(window).scrollTop(90); $(document).scrollTop(90); 
		});
		$(".online_media.word a[title]").removeAttr("title");
		
		if($.fn.placeholder)
			$("[placeholder]").placeholder();
	
		// Update this global var with the right users
		current_users = window.data.users;
	
	
		var showEntry 		= _.template($("#template").html()),
			mediaSelect 	= $("select.media"),
			interestSelect 	= $("select.interest"),
			searchBox		= $("[name=dirSearch]"),
			resultsDiv 		= $("div.results"),
			paginationElement = $(".pagination");

		window.interest = (interestSelect.val() ? window.data.interest[interestSelect.val()] : false);
		window.keyword  = (window.keyword != "(optional)" ? $.trim(searchBox.val()) : "");
		window.media	 = (mediaSelect.val() ? window.data.media[mediaSelect.val()] : false);
	
		function reloadSearch(trackAction, keycode) {
			/**
			*
			* Retreive state from DOM
			*
			*/
			window.interest = (interestSelect.val() ? window.data.interest[interestSelect.val()] : false);
			window.keyword  = (window.keyword != "(optional)" ? $.trim(searchBox.val()) : "");
			window.media	 = (mediaSelect.val() ? window.data.media[mediaSelect.val()] : false);
			
			var dont_search_fields = ["academia", "displayphoto", "homepage", "id", "linkedin", "facebook", "googleplus", "scholar", "researchgate"]
			
			/**
			*
			* Filter users
			*
			*/
			
			// Clone the orginal array of all users
			// http://www.xenoveritas.org/blog/xeno/the-correct-way-to-clone-javascript-arrays
			var users = window.data.users.slice(0);	
			
			// Filter out the users not of the media type
			if(media) {
				users = _.filter(users, function(user) {
					return _.include(media.users, user.id);
				});
			}
			
			// Filter out the users without this interest
			if(interest) {
				users = _.filter(users, function(user) {
					return _.include(interest.users, user.id);
				});
			}
			
			// Lets see if this item contains the keyword specified
			if(keyword && keyword != "(optional)") {
				// 
				// 		
				// var newString = keyword.replace(/\W+/g, ' ');
				// var keywords = newString.split(' ');
				
				users = _.filter(users, function(item) {
					for(var i in item) {
						
						if(	!_.include(dont_search_fields,i) && item[i] && item[i].toString().toLowerCase) {
							if(item[i].toString().toLowerCase().indexOf(keyword.toLowerCase()) != -1)
								return true;
						}
						
					}

					if((item.firstname + " " + item.lastname).toLowerCase().indexOf(keyword.toLowerCase()) != -1)
						return true;
						
					if((item.salutation + " " + item.lastname).toLowerCase().indexOf(keyword.toLowerCase()) != -1)
						return true;
					
					var firstnames = $.trim(item.firstname).split(' ');
					if(firstnames.length == 2) {
						
						if((firstnames[0] + " " + item.lastname).toLowerCase().indexOf(keyword.toLowerCase()) != -1)
							return true;
						
						if((firstnames[1] + " " + item.lastname).toLowerCase().indexOf(keyword.toLowerCase()) != -1)
							return true;
						
					}
					
					
					return false;
				});
			}
			
			// No need sorted orginal
			//users = _.sortBy(users, function(user){ return user.lastname; });
			
			// Update global var
			current_users = users;
				
			// Lets not rigger the loggin of a "change page"
			_showPageSet(1);
			
			if(users.length >= 2) {
				$(".numResults").html(users.length + " results found.");
			} else {
				if(users.length == 1)
					$(".numResults").html("1 result found.");
				else
					$(".numResults").html("No results found -- please try a different search.<br /><br /><br /><br />");
			}
			
			socket.emit('socialmedia', { 
				action: trackAction,
				time: new Date().getTime(),
				keyword: (keyword != "(optional)" ? keyword : ""),
				keycode: keycode,
				num_results: users.length,
				media: (media ? media.field : null),
				interest: (interest ? interest.name : null),
				page: 1,
				referrer: document.referrer,
				agent: navigator.userAgent,
				platform: navigator.platform,
				cookieEnabled: navigator.cookieEnabled
			});
		
		}
		
		var logEvent = _.debounce(function(pageNum) {
			try {
			var log_data = ['_trackEvent', 'Social Media Directory Search', JSON.stringify([(window.interest ? window.interest.name : null), (window.media ? window.media.name : null)]), (window.keyword ? window.keyword : ""), (pageNum ? pageNum : null)];
			
			if(window._gaq && _gaq.push)
				_gaq.push(log_data);
			else
				console.log(log_data);
			
			} catch(r) {}
			
		}, 300);
		
		var logEvent2 = _.debounce(function(pageNum) {
			
			try {
			var log_data = ['_trackEvent', 'Social Media Directory Search - 1second', JSON.stringify([(window.interest ? window.interest.name : null), (window.media ? window.media.name : null)]), (window.keyword ? window.keyword : ""), (pageNum ? pageNum : null)];
			
			if(window._gaq && _gaq.push)
				_gaq.push(log_data);
			else
				console.log(log_data);
				
			} catch(r) {}
			
		}, 1000);
		
		var logEvent3 = _.debounce(function(pageNum) {
			
			try {
			
			var log_data = ['_trackEvent', 'Social Media Directory Search - .5second', JSON.stringify([(window.interest ? window.interest.name : null), (window.media ? window.media.name : null)]), (window.keyword ? window.keyword : ""), (pageNum ? pageNum : null)];
			
			if(window._gaq && _gaq.push)
				_gaq.push(log_data);
			else
				console.log(log_data);
				
			} catch(r) {}
			
		}, 500);
		
		
		window.showPageSet = function(pageNum) {
		
			window._showPageSet(pageNum);
		

			socket.emit('socialmedia', { 
				action: "change:page",
				keyword: (window.keyword != "(optional)" ? $.trim(searchBox.val()) : ""),
				time: new Date().getTime(),
				num_results: current_users.length,
				media: (window.media ? window.media.field : null),
				interest: (window.interest ? window.interest.name : null),
				page: pageNum,
				referrer: document.referrer,
				agent: navigator.userAgent,
				platform: navigator.platform,
				cookieEnabled: navigator.cookieEnabled
			});
			
		};
		
		window._showPageSet = function(pageNum) {

			logEvent(pageNum);
			logEvent2(pageNum);
			logEvent3(pageNum);

			// IE7 does nto see the .pagination click event, so here we move the screen up.
			if($("html.ie7,html.ie8").length > 0) {
				$("html,body").scrollTop(90); $(window).scrollTop(90); $(document).scrollTop(90); 
			}

			// Lets make up the results
			
			var element = document.createElement("div"),
				viewable_users = _.first(_.rest(current_users, (pageNum-1)*window.data.pagination), window.data.pagination);
			
			
			var count = 0;
			_.each(viewable_users, function(user) {
				count++;
				var item = $(showEntry(user));
				
				var media = ['facebook', 'twitter','academia', 'researchgate', 'linkedin', 'googleplus', 'scholar'];
				
				var num_media = media.length;
				
				_.each(media, function(j) {
					if(!user[j]) {
						item.find(".icon."+j).parent().remove();	
						num_media--;
					}
				});
				
				if(num_media > 3 || $("html").hasClass("ie7")) {
					item.find(".online_media .icon").html("");
					item.find(".online_media").removeClass("word");
				} else {
					item.find(".online_media").addClass("word");
					item.find(".online_media a[title]").removeAttr("title");
				}
				
				if(!user.homepage) {
					item.find("a.homepage").next().remove();
					item.find("a.homepage").remove();
				}
				
				
				if(count == viewable_users.length)
					item.addClass("last");
				
				if(parseInt(user.displayphoto) && parseInt(user.photo_uploadid)) {
					// do nothing the photo is there
				} else {
					// if there is a reason not to show lets not show it.
					item.find("img").attr("src", "http://socialpsychology.org/images/nophoto75x90.jpg").show();
				}
				
				$(element).append(item);
				
			});
			
			resultsDiv.html($(element).html());
			
			resultsDiv.find("a").click(function() {
				socket.emit('socialmedia', { 
					action: "result",
					profile_holder: $(this).closest(".entry").find("h2").text(),
					result_url: $(this).attr("href"),
					time: new Date().getTime(),
					keyword: (window.keyword != "(optional)" ? $.trim(searchBox.val()) : ""),
					num_results: current_users.length,
					media: (window.media ? window.media.field : null),
					interest: (window.interest ? window.interest.name : null),
					page: pageNum,
					referrer: document.referrer,
					agent: navigator.userAgent,
					platform: navigator.platform,
					cookieEnabled: navigator.cookieEnabled
				});	
			});
			
			if(!viewable_users || viewable_users.length == 0)
				$(".box").hide();
			else
				$(".box").show();

			window.resetTemplateHeight();
			
			
			// Lets do the pagination
			
			var numPages = Math.ceil(current_users.length/window.data.pagination);
				html = [],
				j = 0;
				
			if(pageNum != 1 && numPages > 10)
				html[j++] = " <a onclick='showPageSet("+1+")'><span class='carrotB'>&laquo;</span>First</a> ";
			
			if(pageNum > 1)
				html[j++] = "<a onclick='showPageSet("+(pageNum-1)+")' style='margin-left:10px'><span class='carrotB'>&lsaquo;</span>Previous</a> ";
			
			var numbers_html = [], h = 0, numbers_shown = 10;
			
			if(pageNum - (numbers_shown / 2) > 1) {
				
				if(pageNum + (numbers_shown / 2) <= numPages) {
					startCounter = pageNum - (numbers_shown / 2);
					numbers_html[h++] = "<span>...</span>";
				} else {
					if(numPages - numbers_shown < 0) {
						startCounter = 1;
					} else {
						startCounter = numPages - (numbers_shown - 1);
						numbers_html[h++] = "<span>...</span>";
					}
				}
			} else {
				startCounter = 1;
			}
			
			if(numPages > 1) {
				for(var i = startCounter; i <= numPages; i++) {
				
					if(i >= startCounter + numbers_shown) {
						numbers_html[h++] = "<span>...</span>";
						break;
					}
				
					if(pageNum == i)
						numbers_html[h++] = "<strong>"+i+"</strong>";
					else
						numbers_html[h++] = "<a onclick='showPageSet("+i+")'>" + i + "</a>";
				
				}
			}
			
			
			html[j++] = numbers_html.join(" ");
			
			if(pageNum < numPages)
				html[j++] = " <a onclick='showPageSet("+(pageNum+1)+")' style='margin-right:10px'>Next<span class='carrotB'>&rsaquo;</span></a>";
			
			if(pageNum != numPages && numPages > 10)
				html[j++] = " <a onclick='showPageSet("+numPages+")'>Last<span class='carrotB'>&raquo;</span></a>";
			
			paginationElement.html(html.join(''));
			
		};
		
		interestSelect.change(function() {
			reloadSearch("change:interest");
		});
		
		mediaSelect.change(function() {
			reloadSearch("change:media");
		});
		
		if($("html.ie8,html.ie7").length > 0) {
			searchBox.keyup(_.debounce(function(e) {
				var keyCode = (window.event) ? e.which : e.keyCode;
				reloadSearch("change:search", keyCode);
			}, 500));
		} else {
			searchBox.keyup(function(e) {
				var keyCode = (window.event) ? e.which : e.keyCode;
				reloadSearch("change:search", keyCode);	
			});
		}
		
	});

</script>

<div class="basic">
	<div class="topHeader">
		
		<h2>Search the SPN Social Media Directory</h2>
		<div class="renewBox">
			<div class="topRight"></div>
			<h1>Other Search Options</h1>
			<div class="content">
				<p>
					<img src="/images/arrow_sm_blue.gif" width="6" height="7" borde="0"> <a href="http://www.socialpsychology.org/profilesearch.htm">Profile Directory</a><br />
					<img src="/images/arrow_sm_blue.gif" width="6" height="7" borde="0"> <a href="http://www.socialpsychology.org/profilemap.htm">Global GeoSearch</a><br />
					<img src="/images/arrow_sm_blue.gif" width="6" height="7" borde="0"> <a href="http://www.socialpsychology.org/maps/researchgroups/">Research Groups </a><br />
					<img src="/images/arrow_sm_blue.gif" width="6" height="7" borde="0"> <a href="http://www.socialpsychology.org/maps/gradprograms/">Graduate Programs </a><br />
					<img src="/images/arrow_sm_blue.gif" width="6" height="7" borde="0"> <a href="http://www.socialpsychology.org/asearch.htm">Advanced Search </a>
				</p>		
			</div>
		</div>
		<p>
			Want to send a Facebook friend request to a colleague, follow Twitter postings on a specific research topic, or see who's listed in Google Scholar? Use the options below to search more than <strong>1,300</strong> SPN Profile holders who have links to social media pages. 
		</p>
		<br />
		<div style="float:left">
			<div>
				Name or keyword:
				<input type="text" name="dirSearch" placeholder="(optional)" size="56">
			</div>
