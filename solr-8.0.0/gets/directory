	<!-- <script src="secure.socialpsychology.org/js/libs/respond/respond.min.js"></script>
    <link href="/js/libs/respond/respond-proxy.html" id="respond-proxy" rel="respond-proxy" />
    <link href="secure.socialpsychology.org/js/libs/respond/respond.proxy.gif" id="respond-redirect" rel="respond-redirect" />	
	<script src="secure.socialpsychology.org/js/libs/respond/respond.proxy.js"></script> -->
	<script>
		/**
		*
		* Add time to the template
		*
		*
		*/

		/* preload Images */
		var preloadImages = ["/img/template/default/supportbtn-on.gif", "/img/template/default/nav_directory_search_o.gif", "//www.socialpsychology.org/images/nav_directory_home_o.gif"];

		for(var i in preloadImages) {
			var pic = new Image(); 
			pic.src = preloadImages[i];	
		}


		var currentDate = new Date(),
			daysOfWeek = ["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday","Sunday"],
			months = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
			dayOfWeek = daysOfWeek[currentDate.getDay()],
			month = months[currentDate.getMonth()],
			dateNumber = currentDate.getDate(),
			dateEnding = "th";

			switch(dateNumber % 10) {

				case 1:
					if(dateNumber == 11)
						dateEnding = "th";
					else
						dateEnding = "st";
					break;

				case 2:
					if(dateNumber == 12)
						dateEnding = "th";
					else
						dateEnding = "nd";
					break;

				case 3:
					if(dateNumber == 13)
						dateEnding = "th";
					else
					dateEnding = "rd";
					break;

				default:
					ending = "th";

			}

		// Output Current Time
		document.getElementById("time_header").innerHTML = dayOfWeek + ", " + month + " " + dateNumber + dateEnding + ", " + currentDate.getFullYear();


		/**
		*
		* Fix for the tempalt not extending all the way to the bottom
		*
		*/
		window.resetTemplateHeight = function() {
			
			try {
				$("#sectionBody").css({'height': 'auto'});

				if($("#sectionBody").height() < $("body").height() )
					$("#sectionBody").height($("body").height());
			} catch(e) {
				var sectionBody = document.getElementById("sectionBody");
				sectionBody.style.height = 'auto';
				if(sectionBody.clientHeight < document.body.clientHeight) sectionBody.style.height = (document.body.clientHeight +50) + "px";
			}
				
		};
		loader.ready(function() {
			window.resetTemplateHeight();
		});
		
		setTimeout(function() {
			window.resetTemplateHeight();
		}, 500);
	</script>
	
		
