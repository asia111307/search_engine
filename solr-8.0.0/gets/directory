		background: transparent url('/img/social_networking_iconpack/academiaEdu.png') 2px 2px no-repeat;
	}

	.media .key3, .media-selectBox-dropdown-menu .key3 a {
                background: transparent url('/img/social_networking_iconpack/researchgate.png') 2px 2px no-repeat;
        }
	
	.media .key4, .media-selectBox-dropdown-menu .key4 a {
		background: transparent url('/img/social_networking_iconpack/linkedin_16.png') 2px 2px no-repeat;
	}
	
	.media .key5, .media-selectBox-dropdown-menu .key5 a {
		background: transparent url('/img/social_networking_iconpack/gplus-16.png?v=2') 2px 2px no-repeat;
	}
	
	.media .key6, .media-selectBox-dropdown-menu .key6 a {
		background: transparent url('/img/social_networking_iconpack/google_16.png') 2px 2px no-repeat;
	}
	
