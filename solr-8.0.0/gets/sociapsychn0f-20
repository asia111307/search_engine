    padding: 10px;
    width: 800px;
    box-shadow: 0 5px 8px 0 rgba(0,0,0,0.2), 0 7px 20px 0 rgba(0,0,0,0.2);
}

.faw-modal-body {
   border: none;
   text-align: center;
}

.faw-carousel-buttons {
   height: 40px;
   width: auto;
   display: inline;
   opacity: 1;
   position: absolute;
   z-index: 1;
   top: 48px;
   box-shadow: 0 3px 5px #888;
}

