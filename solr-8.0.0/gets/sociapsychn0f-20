                                     return {"trigger":"ns_HSA5F5CSRG8ZM8HWQQ26_1837_1_hmd_pricing_feedback_trigger_product-detail","destination":"/gp/pdp/pf/pricingFeedbackForm.html?ie=UTF8&%2AVersion%2A=1&%2Aentries%2A=0&ASIN=155798395X&PREFIX=ns_HSA5F5CSRG8ZM8HWQQ26_1837_2_&WDG=book_display_on_website&dpRequestId=HSA5F5CSRG8ZM8HWQQ26&from=product-detail&storeID=booksencodeURI('&originalURI=' + window.location.pathname)","url":"/gp/pdp/pf/pricingFeedbackForm.html?ie=UTF8&%2AVersion%2A=1&%2Aentries%2A=0&ASIN=155798395X&PREFIX=ns_HSA5F5CSRG8ZM8HWQQ26_1837_2_&WDG=book_display_on_website&dpRequestId=HSA5F5CSRG8ZM8HWQQ26&from=product-detail&storeID=books","nsPrefix":"ns_HSA5F5CSRG8ZM8HWQQ26_1837_2_","path":"encodeURI('&originalURI=' + window.location.pathname)","title":"Tell Us About a Lower Price"};
