<script>
  // In production, we may need to submit forms across sub-domains.
  // The easiest way to do that is to tell the browser to treat us
  // as if we're at computer.org instead of *.computer.org. This only
  // works when on a sub-domain of a target domain
  // if (window.location.href.indexOf('computer.org') > -1) {
  //     document.domain = 'computer.org';
  // }
  // The following function allows an article to have click-able reference
  // links that will scroll to the 'References' section of the article
  // and highlight the appropriate reference
  function scrollToReference(referenceNum) {
    var refQuery = '#' + referenceNum;
    $('html, body').animate({
      scrollTop: $(refQuery).offset().top,
    }, 1000);
    $('ul.referencesNotesList').children()
      .css('background', 'inherit');
    $(refQuery).parent('li')
      .css('background', '#ffffd1');
  }
  // When the user scrolls down 20px from the top of the document, show the
  // scroll-to-top button
  window.onscroll = function() {scrollFunction()};
  function scrollFunction() {
    const scrollToTopLink = $('#scrollToTopLink');
    if (scrollToTopLink) {
      if (document.body.scrollTop > 600 ||
        document.documentElement.scrollTop > 600) {
        scrollToTopLink.css('display', 'block');
      } else {
        scrollToTopLink.css('display', 'none');
      }
    }
  }
  // When the user clicks on the button, scroll to the top of the document
  function scrollToTopOfPage() {
    $('html, body').animate({
      scrollTop: $('html').offset().top,
    }, 1000);
  }
</script>
