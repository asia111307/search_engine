<script type="text/javascript">
  MathJax.Hub.Config({
    mml2jax: {
      preview: "none"
    },
    tex2jax: {
      inlineMath: [
        ['$_$','$_$'],
        ['\\(','\\)'],
        ['__$','$__'],
        ['$$','$$'],
        ['Z_$','$_Z'],
        ['$','$']
      ]
    },
    showProcessingMessages: false,
    messageStyle: "none",
    "fast-preview": {
      disabled: true
    },
    CommonHTML: {
      linebreaks: {
        automatic: true
      }
    },
    "HTML-CSS": {
      linebreaks: {
        automatic: true
      }
    },
    SVG: {
      linebreaks: {
        automatic: true
      }
    },
    TeX: {
      noErrors: {
        disabled: true
      }
    },
    MathMenu: {
      styles: {
        ".MathJax_Menu": {
          "z-index": 2001
        }
      },
      delay: 600
    },
    menuSettings: {
      zoom: "Click"
    }
  });
  MathJax.Hub.Queue(function () {
    var mathMLTally = $(".mjx-chtml").size();
    if ( mathMLTally > 0 ) {
      $.gritter.add({
        title: 'MathML, TeX, ASCIImath Found',
        text: 'It looks like there are some MathML/TeX/ASCIImath formulas in this article.<br/><br/>Right-clicking on each formula will present a menu with options from which you can choose.',
        sticky: true,
        class_name: 'gritter-dark'
      });
    }
  });
</script>
