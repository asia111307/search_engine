                $(".collapse").on('show.bs.collapse', function(){
            var $this = $(this);
            FB.XFBML.parse(document.getElementById('fb-comments-0'), function() {
                var fbSuccess = setInterval(function(){
                    if($('#fb-comments-0').attr('fb-xfbml-state') == 'rendered')
                    {
                        clearInterval(fbSuccess);
                        $('.comment-loading',$this).hide();
                        $('.fb-comments-wrp',$this).show();
                    }
                }, 1000);
            });
        });
            $(function() {
        var categoryMenu = $('.category-menu');

        if($('.sub-category-list').length)
        {
            if ($(window).width() > 1014)
            {
                $('.sub-category-list').width($(window).width() - $('.category-name').width()).show();
                categoryMenu.width($(window).width() - $('.category-name').width()-10);
            }
            else
            {
                $('.sub-category-list').width($(window).width() - $('.category-name').width()-20).show();
                categoryMenu.width($(window).width() - $('.category-name').width()-80);
            }

            var sly = new Sly(categoryMenu, {
                horizontal: 1,
                itemNav: 'centered',
                activateOn: 'click',
                mouseDragging: 1,
                touchDragging: 1,
                releaseSwing: 1,
                activatePageOn: 'click',
                speed: 300,
                elasticBounds: 1,
                dragHandle: 1,
                dynamicHandle: 1,
                clickBar: 1,
                prevPage: $('.controls').find('.prevPage'),
                nextPage: $('.controls').find('.nextPage')
            }).init();

            var $pos = 0;

            sly.on('moveStart', function (eventName) {
                if($pos < sly.pos.dest)
                {
                    setEvent('Category Navigation Bar','Right Arrow','Click');
                    $pos = sly.pos.dest;
                }
                else if($pos > sly.pos.dest)
                {
                    setEvent('Category Navigation Bar','Left Arrow','Click');
                    $pos = sly.pos.dest;
                }
            });

            $('.category-menu > ul > li > a').on('click',function(){
                setEvent('Category Navigation Bar',$(this).text(),'Click');
            });
        }

        if($('.sub-category-list').length)
        {
            $( window ).resize(function() {
                if ($(window).width() > 1014)
                {
                    $('.sub-category-list').width($(window).width() - $('.category-name').width());
                    categoryMenu.width($(window).width() - $('.category-name').width()-10);
                    sly.reload();
                }
                else
                {
                    $('.sub-category-list').width($(window).width() - $('.category-name').width()-20);
                    categoryMenu.width($(window).width() - $('.category-name').width()-80);
                    sly.reload();
                }
            });
        }
    });

                                $(window).on("load", function() {
                    $('#sticky__sidebar').height($(document).height() - ($('#site-footer').height() + 234));
                });
                    
        $("#share1_0").jsSocials({
            url: "https://interestingengineering.com/cognitive-computing-more-human-than-artificial-intelligence",
            id: "share1_0",
            showLabel: false,
            showCount: false,
            shares: [
                "facebook",
                "twitter",
                "linkedin",
                "pinterest",
                {
                    renderer: function() {
                        $result = $("<div>");
                        $result.addClass('jssocials-share jssocials-share-reddit');

                        $a = $("<a>");
                        $('<i>').addClass('fa fa-reddit-alien jssocials-share-logo').appendTo($a);
                        $('<span>').addClass('jssocials-share-count').appendTo($a);

                        $a.addClass("jssocials-share-link jssocials-share-link-count jssocials-share-no-count")
                            .attr("href", 'https://www.reddit.com/submit?url=https%3A%2F%2Finterestingengineering.com%2Fcognitive-computing-more-human-than-artificial-intelligence')
                            .attr("target", "_blank")
                            .appendTo($result);

                        return $result;
                    }
                }],
            text: "Cognitive Computing: More Human Than Artificial Intelligence"
        });

        var iasPush = jQuery.ias({
            container:  '.ias-push',
            item:       '.ias-push-item',
            pagination: '#next-content',
            next:       '.next-content',
            delay:      0,
            negativeMargin: 200
        });

        iasPush.extension(new IASPagingExtension());
        iasPush.extension(new IASHistoryExtension({ prev: '.prev a' }));
        iasPush.extension(new IASNoneLeftExtension({
            html: '<div class="container tb-margin-10"><a disabled class="btn btn-lg btn-primary load-more black-bottom-shadow">{text}</a></div>'
        }));
        iasPush.extension(new IASSpinnerExtension({
            src: 'https://static.interestingengineering.com//assets/img/loading-blocks.svg'
        }));
        iasPush.extension(new IASTriggerExtension({
            html: '<div class="container tb-margin-10"><a class="btn btn-lg btn-primary load-more black-bottom-shadow">NEXT ARTICLE</a></div>'
        }));
        iasPush.on('load', function(event) {
            c = c+1;
            event.url = event.url + "?c="+c;
        });

        iasPush.on('rendered', function(items) {
            $(items).each(function() {
                var $this = this;
                var $contentData = $($this).find('.content-title');
                var url = $contentData.data('url');
                var title = $contentData.text();
                
                var totalSharesCounts = {};

                var refreshTotalCount = function(shareId) {
                    $("#totalCount_" + shareId,$this).text(jsSocials.Socials.prototype._formatNumber(totalSharesCounts[shareId]));
                };

                var origGetCountValue = jsSocials.Socials.prototype._getCountValue;

                jsSocials.Socials.prototype._getCountValue = function(response, share) {
                    var count = share.getCount(response);
                    totalSharesCounts[this.id] = (totalSharesCounts[this.id] ? totalSharesCounts[this.id] : 0) + (parseInt(count, 10) || 0);
                    refreshTotalCount(this.id);
                    return origGetCountValue.apply(this, arguments);
                };

                $("#share1_"+c,this).jsSocials({
                    url: url,
                    id: "share1_"+c,
                    showLabel: false,
                    showCount: false,
                    shares: ["facebook",
                            "twitter",
                            "linkedin",
                            "pinterest",
                            {
                                renderer: function() {
                                    redditUrl = 'https://www.reddit.com/submit?url='+encodeURIComponent(url);
                                    $result = $("<div>");
                                    $result.addClass('jssocials-share jssocials-share-reddit');

                                    $a = $("<a>");
                                    $('<i>').addClass('fa fa-reddit-alien jssocials-share-logo').appendTo($a);
                                    $('<span>').addClass('jssocials-share-count').appendTo($a);

                                    $a.addClass("jssocials-share-link jssocials-share-link-count jssocials-share-no-count")
                                        .attr("href", redditUrl)
                                        .attr("target", "_blank")
                                        .appendTo($result);

                                    return $result;
                                }
                            }],
                    text: title
                });

                myLazyLoad.update();

                                $(".collapse",$this).on('show.bs.collapse', function(){
                    FB.XFBML.parse(document.getElementById('#fb-comments-'+c), function() {
                        console.log('waiting');
                        var fbSuccess = setInterval(function(){
                            console.log('trying');
                            if($('#fb-comments-'+c).attr('fb-xfbml-state') == 'rendered')
                            {
                                console.log('success');
                                clearInterval(fbSuccess);
                                $('.comment-loading',$this).hide();
                                $('.fb-comments-wrp',$this).show();
                            }
                        }, 1000);
                    });
                });
                                                OBR.extern.researchWidget();
                            });

                                            $('#sticky__sidebar').height($(document).height());
                                                        setEvent('Article','Next Article Button','Click');
                dataLayer.push(gtmData);
                    });

            </script>
