<script src="https://www.rapidvaluesolutions.com/wp-content/themes/RapidValue/js/wow.min.js"></script>
<script>
	new WOW().init();

	jQuery(function ($) {
		$(window).resize(function () {
			if ($(window).width() < 768) {
				$(".dropdown-toggle").attr('data-toggle', 'dropdown');
			} else {
				$(".dropdown-toggle").removeAttr('data-toggle dropdown');
			}
			if ($(window).width() < 1280) {}
		});
	});

	var fadeblock = $('.innerbanner');
	var range = 200;
	if (fadeblock.length) {
		$(window).on('scroll', function () {

			var scrollTop = $(this).scrollTop();
			var offset = fadeblock.offset().top
			var height = fadeblock.outerHeight();
			offset = offset + height / 2;
			var calc = 1 - (scrollTop - offset + range) / range;

			fadeblock.css({
				'opacity': calc
			});

			if (calc > '1') {
				fadeblock.css({
					'opacity': 1
				});
			} else if (calc < '0') {
				fadeblock.css({
					'opacity': 0
				});
			}
		});
	}

	$('.footer_menu_head').click(function (e) {
		if (jQuery(window).width() < 768) {
			e.preventDefault();
			var $this = $(this).parent().find('.sub-menu');
			$(".footer-nav .sub-menu").not($this).hide();
			$this.slideToggle();
		}
	});

	/*$(window).resize(function() {
		location.reload();
	});*/


	$(".menu-item").on("click", function () {
		if ($(".sub-menu .rvs-back:visible")) {
			$('.menu_whole .rvs-back').css('display:none !important;');
		}
	});
</script>
